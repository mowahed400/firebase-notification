{
    "name": "waheed/firebase-notification",
    "description": "Laravel package for Firebase Cloud Messaging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Waheed",
            "email": "elgohary.wahed@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Waheed\\FirebaseNotification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waheed\\FirebaseNotification\\FirebaseNotificationServiceProvider"
            ],
            "aliases": {
                "FirebaseNotification": "Waheed\\FirebaseNotification\\Facades\\FirebaseNotification"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}