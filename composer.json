{
    "name": "mowahed/firebase-notification",
    "description": "Laravel package for Firebase Cloud Messaging",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Waheed",
            "email": "elgohary.wahed@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0 <8.4",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
        ],
        "autoload": {
            "psr-4": {
                "Mowahed\\FirebaseNotification\\": "src/"
            }
        },
        "extra": {
            "laravel": {
                "providers": [
                    "Mowahed\\FirebaseNotification\\FirebaseNotificationServiceProvider"
                ],
                "aliases": {}
            }
        },
        "minimum-stability": "dev",
        "prefer-stable": true
    }
